{
	"tags": {
		"allowUnknownTags": true
	},
	"plugins": [
		"plugins/markdown",
		"./plugin-module-type.js"
	],
	"templates": {
		"cleverLinks": false,
		"monospaceLinks": false,
		"default": {
			"outputSourceFiles": false,
			"includeDate": false
		}
	},
	"markdown": {
		"parser": "gfm",
		"hardwrap": true
	},
	"source": {
		"include": [
			"../../tmkt.js"
		]
	}
}